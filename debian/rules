#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

# This has to be exported to make some magic below work.
export DH_OPTIONS

%:
	dh $@ 

build:
	make

override_dh_auto_install:
	mkdir -p debian/dwm-luwrain/usr/bin
	install -m 755 dwm debian/dwm-luwrain/usr/bin/dwm-luwrain
